{
  "name": "ibrand/advert",
  "type": "library",
  "description": "ibrand advert component",
  "keywords": [
    "iBrand",
    "advert",
    "component"
  ],
  "require": {
    "php": ">=7.0",
    "prettus/l5-repository": "^2.1",
    "kalnoy/nestedset":"~4.3"
  },
  "require-dev": {
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~6.0",
    "orchestra/testbench": "~3.5",
    "orchestra/database": "~3.5"
  },
  "autoload": {
    "psr-4": {
      "iBrand\\Component\\Advert\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "iBrand\\Component\\Advert\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "iBrand\\Component\\Advert\\AdvertServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}